syntax = "proto3";

message SphinxPacket {
    Header Hdr = 1;
    bytes Payload = 2;
}

message Header {
    bytes Alpha = 1;
    bytes Beta = 2;
    bytes Gamma = 3;
}

message Hop {
    bytes MultiAddress = 1;
}

message RoutingInfo {
    Hop Addr = 1;
    bytes Delay = 2;
    bytes Gamma = 3;
    bytes Beta = 4;
}

message HeaderInitials {
    bytes Alpha = 1;
    bytes Secret = 2;
    bytes Blinder = 3;
}